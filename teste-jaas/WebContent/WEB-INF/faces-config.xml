            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>atualizarTipoDocumento</from-outcome>
            <to-view-id>/pages/protected/admin/atualizarTipoDocumento.xhtml</to-view-id>
