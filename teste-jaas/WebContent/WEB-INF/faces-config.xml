	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	
    <navigation-rule>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/pages/public/main.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>listarDocumentos</from-outcome>
            <to-view-id>/pages/public/listarDocumentos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>incluirDocumento</from-outcome>
            <to-view-id>/pages/protected/users/incluirDocumento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>atualizarDocumento</from-outcome>
            <to-view-id>/pages/protected/users/atualizarDocumento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>deletarDocumento</from-outcome>
            <to-view-id>/pages/protected/users/deletarDocumento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>incluirArquivoAnexo</from-outcome>
            <to-view-id>/pages/protected/users/incluirArquivoAnexo.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
